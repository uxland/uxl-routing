{
  "name": "@uxland/uxl-routing",
  "version": "0.0.0-development",
  "description": "UXLand Routing",
  "homepage": "https://github.com/uxland/uxl-routing#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/uxland/uxl-routing.git"
  },
  "bugs": {
    "url": "https://github.com/uxland/uxl-routing/issues"
  },
  "author": "UXLand <dev@uxland.es> (http://www.uxland.es)",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "keywords": [
    "polymer",
    "web components"
  ],
  "devDependencies": {
    "@olavoparno/jest-badges-readme": "^1.3.6",
    "@polymer/iron-demo-helpers": "^3.0.0-pre.19",
    "@polymer/iron-pages": "^3.0.1",
    "@types/jest": "^24.0.11",
    "@types/ramda": "^0.25.48",
    "@types/redux-mock-store": "^1.0.0",
    "@webcomponents/custom-elements": "^1.2.0",
    "@webcomponents/webcomponentsjs": "^2.0.0",
    "caf": "^7.0.6",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "ejs-loader": "^0.3.5",
    "html-webpack-exclude-assets-plugin": "0.0.7",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.0",
    "jest": "^24.5.0",
    "jest-when": "^2.6.0",
    "jsdom": "^14.0.0",
    "mocha": "^5.2.0",
    "mutation-observer": "^1.0.3",
    "nsp": "^2.6.3",
    "path": "^0.12.7",
    "raf": "^3.4.1",
    "redux-mock-store": "git+https://github.com/dpinart/redux-mock-store.git",
    "rimraf": "^3.0.0",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "semantic-release": "^16.0.0-beta.25",
    "travis-deploy-once": "^5.0.7",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.2.0",
    "typescript": "^3.3.3",
    "wct-browser-legacy": "^1.0.2",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1"
  },
  "dependencies": {
    "@uxland/lit-redux-connect": "^1.0.2",
    "@uxland/uxl-redux": "^3.0.4",
    "@uxland/uxl-utilities": "^3.3.2",
    "lit-element": "^2.1.1",
    "ramda": "^0.26.1",
    "reselect": "^4.0.0",
    "tslib": "^1.9.3"
  },
  "scripts": {
    "deps:delete": "rimraf node_modules package-lock.json",
    "deps:install": "npm install",
    "deps:refresh": "npm run deps:delete && npm run deps:install",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build:es": "tsc -p tsconfig.build.json --outDir es  ",
    "build:cjs": "tsc -p tsconfig.build.json --outDir lib --module commonjs --declaration",
    "build": "npm run build:es && npm run build:cjs",
    "test:ui:watch": "nodemon --watch test --watch src -x \"polymer test --config-file wct.config.json\"",
    "test:ui": "polymer test --config-file wct.config.json",
    "test:unit": "jest --collect-coverage",
    "tdd": "jest --watch",
    "dist": "npm run build",
    "travis-deploy-once": "travis-deploy-once",
    "readme:coverage": "node_modules/.bin/jest-badges-readme",
    "semantic-release": "semantic-release",
    "serve": "webpack-dev-server --hot --inline --progress"
  },
  "directories": {
    "test": "test"
  },
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test:unit; npm run readme:coverage; git add ."
    }
  }
}
